<?xml version="1.0"?>
<launch>
	<!-- FRONT HOKUYO -->
	<node name="hokuyo_front_urg" pkg="urg_node" type="urg_node" respawn="false" output="screen">
		<remap from="/scan" to="/guardian/laser_tilt_front" />
